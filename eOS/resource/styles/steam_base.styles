    	}
		
		Frame
		{
			bgcolor=eOSInactiveWindowBG
			corner_rounding=5
			render_bg [!$OSX]
			{
				0="gradient(x0, y0, x1, y0+32, eOSInactiveTitleTop, eOSInactiveTitleBottom)"
				1="fill(x0, y0+31, x1, y0+32, eOSInactiveTitleBorder)"
				//2="fill(x0, y0+32, x1, y1, eOSInactiveWindowBG)"
				3="fill(x0, y0, x0 + 1, y1, eOSInactiveTitleBorder)"
				4="fill(x0, y0, x1, y0 + 1, eOSInactiveTitleBorder)"
				5="fill(x1 - 1, y0, x1, y1, eOSInactiveTitleBorder)"
				6="fill(x0, y1 - 1, x1, y1, eOSInactiveTitleBorder)"
			}
			render_bg [$OSX]
			{
				0="gradient(x0, y0, x1, y0+24, eOSInactiveTitleTop, eOSInactiveTitleBottom)"
				1="fill(x0, y0+23, x1, y0+24, eOSInactiveTitleBorder)"
				//2="fill(x0, y0+24, x1, y1, eOSInactiveWindowBG)"
				3="fill(x0, y0, x0 + 1, y1, eOSInactiveTitleBorder)"
				4="fill(x0, y0, x1, y0 + 1, eOSInactiveTitleBorder)"
				5="fill(x1 - 1, y0, x1, y1, eOSInactiveTitleBorder)"
				6="fill(x0, y1 - 1, x1, y1, eOSInactiveTitleBorder)"
			}
		}

		Frame:FrameFocus
		{
			bgcolor=eOSWindowBG
			render_bg [!$OSX]
			{
				0="gradient(x0, y0, x1, y0+32, eOSTitleTop, eOSTitleBottom)"
				1="fill(x0, y0+31, x1, y0+32, eOSTitleBorder)"
				//2="fill(x0, y0+32, x1, y1, eOSWindowBG)"
				3="fill(x0, y0, x0 + 1, y1, eOSTitleBorder)"
				4="fill(x0, y0, x1, y0 + 1, eOSTitleBorder)"
				5="fill(x1 - 1, y0, x1, y1, eOSTitleBorder)"
				6="fill(x0, y1 - 1, x1, y1, eOSTitleBorder)"
			}
			render_bg [$OSX]
			{
				0="gradient(x0, y0, x1, y0+24, eOSTitleTop, eOSTitleBottom)"
				1="fill(x0, y0+23, x1, y0+24, eOSTitleBorder)"
				//2="fill(x0, y0+24, x1, y1, eOSWindowBG)"
				3="fill(x0, y0, x0 + 1, y1, eOSTitleBorder)"
				4="fill(x0, y0, x1, y0 + 1, eOSTitleBorder)"
				5="fill(x1 - 1, y0, x1, y1, eOSTitleBorder)"
				6="fill(x0, y1 - 1, x1, y1, eOSTitleBorder)"
			}
		}

		CGamePropertiesDialog
		{
			render_bg [!$OSX]
			{
				0="gradient(x0, y0, x1, y0+32, eOSInactiveTitleTop, eOSInactiveTitleBottom)"
				1="fill(x0, y0+31, x1, y0+32, eOSInactiveTitleBorder)"
				2="fill(x0, y0+57, x1, y1, eOSInactiveWindowBG)"

				3="gradient(x0, y0+32, x1, y0+56, eOSInactiveSubHeaderTop, eOSInactiveSubHeaderBottom)"
				4="fill(x0, y0+56, x1, y0+57, eOSInactiveTitleBorder)"
			}
			render_bg [$OSX]
			{
				0="gradient(x0, y0, x1, y0+24, eOSInactiveTitleTop, eOSInactiveTitleBottom)"
				1="fill(x0, y0+23, x1, y0+24, eOSInactiveTitleBorder)"
				2="fill(x0, y0+49, x1, y1, eOSInactiveWindowBG)"

				3="gradient(x0, y0+24, x1, y0+48, eOSInactiveSubHeaderTop, eOSInactiveSubHeaderBottom)"
				4="fill(x0, y0+48, x1, y0+49, eOSInactiveTitleBorder)"
			}
			render
			{
				1="fill(x0, y0, x0 + 1, y1, eOSInactiveTitleBorder)"
				2="fill(x0, y0, x1, y0 + 1, eOSInactiveTitleBorder)"
				3="fill(x1 - 1, y0, x1, y1, eOSInactiveTitleBorder)"
				4="fill(x0, y1 - 1, x1, y1, eOSInactiveTitleBorder)"
			}
		}
		CGamePropertiesDialog:FrameFocus
		{
			render_bg [!$OSX]
			{
				0="gradient(x0, y0, x1, y0+32, eOSTitleTop, eOSTitleBottom)"
				1="fill(x0, y0+31, x1, y0+32, eOSTitleBorder)"
				2="fill(x0, y0+57, x1, y1, eOSWindowBG)"

				3="gradient(x0, y0+32, x1, y0+56, eOSSubHeaderTop, eOSSubHeaderBottom)"
				4="fill(x0, y0+56, x1, y0+57, eOSTitleBorder)"
			}
			render_bg [$OSX]
			{
				0="gradient(x0, y0, x1, y0+24, eOSTitleTop, eOSTitleBottom)"
				1="fill(x0, y0+23, x1, y0+24, eOSTitleBorder)"
				2="fill(x0, y0+49, x1, y1, eOSWindowBG)"

				3="gradient(x0, y0+24, x1, y0+48, eOSSubHeaderTop, eOSSubHeaderBottom)"
				4="fill(x0, y0+48, x1, y0+49, eOSTitleBorder)"

			}
			render
			{
				1="fill(x0, y0, x0 + 1, y1, eOSTitleBorder)"
				2="fill(x0, y0, x1, y0 + 1, eOSTitleBorder)"
				3="fill(x1 - 1, y0, x1, y1, eOSTitleBorder)"
				4="fill(x0, y1 - 1, x1, y1, eOSTitleBorder)"
			}
		}

