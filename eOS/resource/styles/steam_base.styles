		headertextlarge
        {
            padding=16
            padding-bottom=0
            font-family=basefont
            font-size=24
            textcolor=Text
            font-weight=400
            bgcolor=none
        }
        
        gamedetails-headerlabel
    	{
    		font-size=18
    		font-style="normal"
